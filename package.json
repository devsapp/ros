{
  "name": "RosComponent",
  "version": "dev.0.2",
  "description": "This is ros component for Serverless Devs Tool ",
  "autoInstall": false,
  "homepage": "https://www.serverless-devs.com",
  "author": "Serverless-Devs",
  "contributors": [],
  "license": "MIT",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "Serverless",
    "Framework",
    "Serverless-Devs",
    "Serverless-Tool",
    "Function Compute"
  ],
  "scripts": {
    "start": "npm run watch",
    "watch": "tsc -w",
    "format": "prettier --write src/",
    "prebuild": "rimraf dist && npm run format &&  rimraf node_modules && npm i",
    "build": "esbuild src/index.ts --bundle --log-level=error --minify --platform=node --format=cjs --target=node10.4  --outfile=dist/index.js",
    "prepare": "husky install",
    "postinstall": "curl -sS http://images.devsapp.cn/pop-sdk/fix-sdk.sh | bash"
  },
  "dependencies": {
    "@alicloud/openapi-client": "^0.4.6",
    "@alicloud/ros20190910": "^3.2.26",
    "@serverless-devs/component-interface": "^0.0.3-beta.1",
    "@serverless-devs/utils": "^0.0.8-beta.6",
    "inquirer": "^8.2.6",
    "lodash": "^4.17.21",
    "string-random": "0.1.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.23",
    "esbuild": "^0.13.8",
    "husky": "^8.0.1",
    "prettier": "^2.8.4",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^4.4.2"
  }
}